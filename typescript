]0;ec2-user@ip-172-31-41-98:~/professional-script[ec2-user@ip-172-31-41-98 professional-script]$ more backup
#!/bin/bash

NOW=$(date +"%a")
case $NOW in
	Mon)	
		echo "Full backup";;
	Tue|Wed|Thu|Fri)
		echo "Partial backup";;
	Sat|Sun)	
		echo "No backup";;
	*) ;;
esac
